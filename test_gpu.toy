fn gpu_vector_add(a: [1024]f32, b: [1024]f32) void {
    a[i] = a[i] + b[i];
}

fn gpu_vector_mul(a: [1024]f32, b: [1024]f32) void {
    a[i] = a[i] * b[i];
}

fn main() i32 {
    let a: [1024]f32 = [1024]f32{2.0f32};
    let b: [1024]f32 = [1024]f32{3.0f32};
    gpu_vector_add(a, b);
    gpu_vector_mul(a, b);
    return 0;
} 
