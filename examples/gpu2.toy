fn gpu_vector_add(a: [1024]i32, b: [1024]i32) void{
    a[i] = a[i] + b[i];
}

fn gpu_vector_mul(a: [1024]i32, b: [1024]i32) void{
    a[i] = a[i] * b[i];
}

fn main() i32 {
    let a: [1024]i32 = [1024]i32{2i32};
    let b: [1024]i32 = [1024]i32{4i32};
    gpu_vector_add(a, b); // a[0] should be 6 after this
    gpu_vector_mul(a, b); // a[0] should be 24 after this
    return a[0]; // should return 24
} 
